{
  {{- if eq .chezmoi.os "linux" }}
  "workbench.colorTheme": "Tokyo Night",
  {{- else if eq .chezmoi.os "windows"}}
  "workbench.colorTheme": "Tokyo Night",
  {{- else if eq .chezmoi.os "darwin"}}
  "workbench.colorTheme": "Tokyo Storm Gogh",
  {{- end }}
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "[Material Theme Darker]": {}
  },
  "telemetry.telemetryLevel": "off",
  "editor.lineNumbers": "relative",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.terminal.launchArgs": ["-m", "IPython"],
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=88",
    "--max-complexity=10",
    "--select=C,E,F,W,B,B950",
    "--extend-ignore=E203,W503",
    "--extend-exclude=*pyi"
  ],
  "isort.args": ["--profile=black"],
  "python.formatting.blackArgs": ["--experimental-string-processing"],
  "djlint.enableLinting": false,
  "djlint.formatJs": true,
  "djlint.ignore": ["H030,H031"],
  "[django-html]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[handlebars]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[hbs]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[mustache]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[jinja]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[jinja-html]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[nj]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[njk]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[nunjucks]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[twig]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "jupyter.askForKernelRestart": false,
  "git.confirmSync": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.rulers": [88],
  "git.enableSmartCommit": true,
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "files.associations": {
    "**/templates/**/*.{html,htm}": "django-html",
    "/requirements{/,*}.{txt,in}": "pip-requirements",
    "*.html": "html",
    "*.lua.tmpl": "lua"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.venv": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/*.egg-info": true,
    "**/.eggs": true
  },
  "emmet.optimizeStylesheetParsing": false,
  "emmet.includeLanguages": {
    "django-html": "html",
    "postcss": "css"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
