{
  "workbench.colorTheme": "Material Theme Palenight High Contrast",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "[Material Theme Darker]": {}
  },
  "telemetry.telemetryLevel": "off",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.terminal.launchArgs": ["-m", "IPython"],
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=88",
    "--max-complexity=10",
    "--select=C,E,F,W,B,B950",
    "--extend-ignore=E203,W503",
    "--extend-exclude=*pyi"
  ],
  "python.sortImports.args": ["--profile=black"],
  "python.formatting.blackArgs": ["--experimental-string-processing"],
  "djlint.formatJs": true,
  "djlint.ignore": ["H030,H031"],
  "[django-html]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[handlebars]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[hbs]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[mustache]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[jinja]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[jinja-html]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[nj]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[njk]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[nunjucks]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[twig]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "jupyter.askForKernelRestart": false,
  "git.confirmSync": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.rulers": [88],
  "git.enableSmartCommit": true,
  "aws.profile": "profile:makane",
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "files.associations": {
    "**/templates/**/*.{html,htm}": "django-html",
    "/requirements{/,*}.{txt,in}": "pip-requirements",
    "*.html": "html",
    "*.lua.tmpl": "lua"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.venv": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/*.egg-info": true,
    "**/.eggs": true
  },
  "emmet.optimizeStylesheetParsing": false,
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "appmap",
    "aramex",
    "autoindent",
    "boto",
    "chezmoi",
    "chezmoitemplates",
    "cpptools",
    "djangorestframework",
    "djlint",
    "esbenp",
    "Fira",
    "isort",
    "karim",
    "Khaleel",
    "loguru",
    "MakanE",
    "middlewares",
    "monosans",
    "multitenancy",
    "mypy",
    "Palenight",
    "paymennt",
    "psycopg",
    "pycache",
    "pyjwt",
    "pyproject",
    "pytest",
    "Redrive",
    "Releans",
    "samclisourcebucket",
    "setuptools",
    "simplejwt",
    "sourcery",
    "Stylesheet",
    "tmpl",
    "venv",
    "vetrina",
    "viewsets",
    "zain"
  ],
  "window.zoomLevel": 1,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "redhat.telemetry.enabled": false,
  {{- if eq .chezmoi.os "linux" }}
  "editor.fontFamily": "'SauceCodePro NFM'"
  {{- end }}
  {{- if eq .chezmoi.os "windows"}}
  "editor.fontFamily": "'FiraMono NFM'"
  {{- end }},
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
